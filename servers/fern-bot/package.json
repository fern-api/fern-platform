{
  "name": "@fern-platform/fern-bot",
  "version": "0.0.0",
  "scripts": {
    "compile": "tsc --build",
    "compile:github-webhook": "esbuild --bundle --target=node18 --platform=node src/functions/github-webhook-listener/githubWebhookListener.ts --outfile=lib/githubWebhookListener.js",
    "run:local:github-webhook": "ts-node ./scripts/run-probot.ts",
    "package": "sls package",
    "release": "sls deploy",
    "invoke": "sls invoke",
    "test:ete": "vitest --run --passWithNoTests --globals",
    "lint": "eslint --max-warnings 0 src --ext .ts",
    "format": "prettier --write --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "lint:fix": "pnpm lint --fix",
    "proxy": "smee -u https://smee.io/3DXoSvCO2NH87w8e",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../.eslintignore",
    "lint:eslint:fix": "pnpm lint:eslint --fix"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "_moduleAliases": {
    "@functions": "lib/src/functions",
    "@generated": "lib/src/generated",
    "@libs": "lib/src/libs",
    "@utils": "lib/src/utils"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.335.0",
    "@fern-api/core-utils": "0.15.0-rc63",
    "@fern-api/github": "workspace:*",
    "@fern-api/venus-api-sdk": "0.8.1-1-gd6d1a5b",
    "@fern-fern/generators-sdk": "0.114.0-eff87265e",
    "@fern-fern/paged-generators-sdk": "0.114.0-eff87265e",
    "@octokit/openapi-types": "^22.1.0",
    "@octokit/webhooks": "^12.0.10",
    "@octokit/webhooks-types": "^7.6.1",
    "@probot/adapter-aws-lambda-serverless": "^4.0.0",
    "@slack/web-api": "^6.9.0",
    "adm-zip": "^0.5.16",
    "cohere-ai": "^7.9.5",
    "execa": "^5.1.1",
    "fern-api": "^0.21.0",
    "find-up": "^7.0.0",
    "js-yaml": "^4.1.0",
    "json-2-csv": "^5.5.1",
    "module-alias": "^2.2.3",
    "octokit": "^3.2.0",
    "probot": "^13.3.8",
    "semver": "^7.6.2",
    "serverless-step-functions": "^3.21.0",
    "simple-git": "^3.24.0",
    "tmp-promise": "^3.0.3",
    "url-join": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@fern-platform/configs": "workspace:*",
    "@octokit/types": "^13.4.1",
    "@serverless/typescript": "^3.0.0",
    "@types/adm-zip": "^0.5.5",
    "@types/aws-lambda": "^8.10.71",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.7.18",
    "@types/semver": "^7.5.8",
    "@types/url-join": "4.0.1",
    "esbuild": "0.24.0",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "smee-client": "^2.0.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "vitest": "^1.5.0"
  },
  "license": "MIT"
}
