})

const chatResponse = await humanloop.chat({
  "project": "sdk-example",
  "messages": [
    {
      "role": "user",
      "content": "Write me a song",
    }
  ],
  "model_config": {
    "model": "gpt-4",
    "temperature": 1,
  },
})

console.log(chatResponse)
