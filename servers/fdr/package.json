  "dependencies": {
    "@aws-sdk/client-s3": "^3.335.0",
    "@aws-sdk/s3-request-presigner": "^3.335.0",
    "@fern-api/fdr-sdk": "0.60.0-1-g4795d27",
    "@fern-api/venus-api-sdk": "^0.1.0",
    "@prisma/client": "5.11.0",
    "@slack/web-api": "^6.9.0",
    "@ungap/url-search-params": "0.2.2",
    "algoliasearch": "^4.22.1",
    "axios": "^1.6.5",
    "axios-logger": "^2.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.1",
    "html-to-text": "^9.0.5",
    "httpsnippet": "^2.0.0",
    "lodash": "^4.17.21",
    "marked": "^5.1.0",
    "moment": "^2.30.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "path-resolver": "*",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^9.0.1",
    "@types/httpsnippet": "^1.23.1",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.194",
    "@types/marked": "^5.0.0",
    "@types/moment": "^2.13.0",
    "@types/node-cache": "^4.2.5",
    "@types/node-cron": "^3.0.8",
    "@types/uuid": "^9.0.1",
    "@yarnpkg/pnpify": "^4.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.2.4",
    "prisma": "5.11.0",
    "tsconfig": "*",
    "typescript": "4.9.4"
  },
  "scripts": {
    "build": "yarn build:prisma && yarn build:compile",
    "build:compile": "tsc --build",
    "build:prisma": "yarn pnpify prisma generate",
    "db:migrate:dev": "dotenv -e .env.dev -- yarn prisma migrate deploy",
    "db:migrate:local": "dotenv -e .env.local -- yarn prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.prod -- yarn prisma migrate deploy",
    "dev": "nodemon src/server.ts ",
    "clean": "rm -rf ./dist && tsc --build --clean",
    "format": "prettier --check \"**/*.{ts,js}\"",
    "format:fix": "prettier --write \"**/*.{ts,js}\"",
    "format:prisma": "prisma format",
    "docker:local": "dotenv -e apps/fdr/.env.local -- ./create_docker.sh local",
    "docker:dev": "dotenv -e apps/fdr/.env.dev -- ./create_docker.sh",
    "docker:prod": "dotenv -e apps/fdr/.env.prod -- ./create_docker.sh",
    "test": "jest src/__test__/unit-tests",
    "lint": "eslint --max-warnings 0 src --ext .ts",
    "lint:fix": "yarn lint --fix",
    "test-db": "dotenv -e .env.test -- ./scripts/db-tests.sh",
    "test-ete": "dotenv -e .env.ete -- ./scripts/ete-tests.sh"
  },
  "packageManager": "yarn@1.22.19"
}
