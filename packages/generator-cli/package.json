{
  "name": "@fern-api/generator-cli",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern-platform.git",
    "directory": "packages/generator-cli"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "bin": {
    "fern-generator-cli": "./dist/cli.cjs"
  },
  "scripts": {
    "clean": "rm -rf ./dist && tsc --build --clean",
    "dist:cli": "tsup ./src/cli.ts --format cjs",
    "test": "vitest --passWithNoTests --globals --disable-console-intercept",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../.eslintignore",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "pnpm lint:style --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck"
  },
  "devDependencies": {
    "@fern-api/fs-utils": "0.15.0-rc63",
    "@fern-fern/generator-exec-sdk": "^0.0.857",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.32",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.15",
    "depcheck": "^1.4.3",
    "esbuild": "^0.15.7",
    "eslint": "^8.56.0",
    "execa": "^5.1.1",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^3.2.4",
    "tmp-promise": "^3.0.3",
    "tsup": "^8.0.2",
    "typescript": "4.9.5",
    "vitest": "^1.5.0",
    "yargs": "^17.4.1"
  }
}
