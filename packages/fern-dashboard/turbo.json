{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "globalEnv": [],
  "tasks": {
    "build": {
      "env": []
    },
    "docs:build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "dashboard:dev": {
      "dependsOn": ["^build", "^compile"]
    },
    "dashboard:start": {
      "dependsOn": ["docs:build"]
    },
    "dashboard:lint": {
      "dependsOn": ["^build", "^compile"]
    }
  }
}
