{
  "name": "@fern-dashboard/ui",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern-platform.git",
    "directory": "packages/fern-dashboard"
  },
  "sideEffects": false,
  "scripts": {
    "compile": "tsc --build",
    "dashboard:build": "next build",
    "dashboard:dev": "next dev",
    "dashboard:dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "dashboard:lint": "next lint",
    "dashboard:start": "next start",
    "depcheck": "depcheck",
    "format": "prettier --write --ignore-unknown \"**\"",
    "format:check": "prettier --check --ignore-unknown \"**\"",
    "lint": "pnpm lint:eslint && pnpm lint:style",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-pattern .next --ignore-pattern '**/*.{m,c,}js'",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "pnpm lint:style --fix",
    "test": "vitest --run --passWithNoTests --globals",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.1.0",
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/s3-request-presigner": "^3.744.0",
    "@fern-api/fdr-sdk": "workspace:*",
    "@fern-api/venus-api-sdk": "^0.13.4-3-g1fe7ae7",
    "@fern-ui/loadable": "workspace:*",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@sparticuz/chromium": "^133.0.0",
    "@tanstack/react-query": "^5.71.1",
    "@tanstack/react-query-devtools": "^5.71.1",
    "@upstash/redis": "^1.34.6",
    "auth0": "^4.20.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "next": "15.3.0-canary.1",
    "next-themes": "^0.4.4",
    "puppeteer-core": "^24.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "^0.33.5",
    "sonner": "^1.5.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@fern-platform/configs": "workspace:*",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^18.11.9",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "tailwindcss": "^4.0.9",
    "typescript": "5.7.2",
    "vitest": "^3.0.5"
  }
}
