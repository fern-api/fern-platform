    "@types/node": "^18.7.18",
    "depcheck": "^1.4.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.7.1",
    "typescript": "4.6.4"
  }
