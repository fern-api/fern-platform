{
  "name": "@fern-api/fdr-sdk",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "source": "src/index.ts",
  "module": "src/index.ts",
  "main": "dist/mjs/index.js",
  "exports": {
    "./navigation": {
      "types": "./dist/mjs/navigation/index.d.ts",
      "import": "./dist/mjs/navigation/index.js",
      "require": "./dist/cjs/nvigation/index.js"
    },
    ".": {
      "types": "./dist/mjs/index.d.ts",
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "dayjs": "^1.11.11",
    "fast-deep-equal": "^3.1.3",
    "form-data": "4.0.0",
    "formdata-node": "^6.0.3",
    "js-base64": "3.7.7",
    "node-fetch": "2.7.0",
    "qs": "6.12.0",
    "tinycolor2": "^1.6.0",
    "title": "^3.5.3",
    "url-join": "4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@fern-api/eslint-config": "workspace:*",
    "@fern-platform/configs": "workspace:*",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node-fetch": "2.6.9",
    "@types/qs": "6.9.14",
    "@types/tinycolor2": "^1.4.6",
    "@types/title": "^3.4.3",
    "@types/url-join": "4.0.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "rollup": "^4.18.0",
    "tsup": "^8.0.2",
    "typescript": "5.4.3",
    "vitest": "^1.5.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap",
    "compile": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && pnpm node scripts/writePackageJson.cjs",
    "clean": "rm -rf ./dist && tsc --build --clean",
    "format": "prettier --write --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "lint": "eslint --max-warnings 0 src --ext .ts --resolve-plugins-relative-to ../../",
    "test": "vitest --run"
  }
}
