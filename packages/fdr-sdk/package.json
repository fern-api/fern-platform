{
  "name": "@fern-api/fdr-sdk",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fern-api/fern-platform.git",
    "directory": "packages/fdr-sdk"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./navigation": {
      "types": "./src/navigation/index.ts",
      "default": "./dist/navigation/index.js"
    },
    "./client/types": {
      "types": "./src/client/types.ts",
      "default": "./dist/client/types.js"
    },
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && tsc --build --clean",
    "compile": "tsc --build",
    "dev": "tsc --watch",
    "docs:dev": "pnpm dev",
    "lint": "eslint --max-warnings 0 src --ext .ts --resolve-plugins-relative-to ../../",
    "test": "vitest --run"
  },
  "dependencies": {
    "dayjs": "^1.11.11",
    "fast-deep-equal": "^3.1.3",
    "form-data": "4.0.0",
    "formdata-node": "^6.0.3",
    "js-base64": "3.7.7",
    "node-fetch": "2.7.0",
    "qs": "6.12.0",
    "tinycolor2": "^1.6.0",
    "title": "^3.5.3",
    "ts-essentials": "^10.0.1",
    "url-join": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@fern-platform/tsconfig": "workspace:*",
    "@types/node-fetch": "2.6.9",
    "@types/qs": "6.9.14",
    "@types/tinycolor2": "^1.4.6",
    "@types/title": "^3.4.3",
    "typescript": "5.4.3",
    "vitest": "^1.5.0"
  }
}
