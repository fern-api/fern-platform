{
  "name": "@fern-ui/app-utils",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern-ui.git",
    "directory": "packages/commons/app-utils"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore --report-unused-disable-directives",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@fern-api/fdr-sdk": "0.50.6-1-gdf25388",
    "@fern-ui/core-utils": "workspace:*",
    "lodash-es": "^4.17.21",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.7.18",
    "depcheck": "^1.4.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^3.2.4",
    "typescript": "4.9.4"
  }
}
