        {
          "key": "description",
          "valueShape": {
            "type": "alias",
            "value": {
              "type": "primitive",
              "value": {
                "type": "string"
              }
            }
          },
          "description": "Description of the voice. This field is required but can be an empty string."
        },
        {
          "key": "is_public",
          "valueShape": {
            "type": "alias",
