{
  "name": "@fern-ui/auth-fe-bundle",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern-ui.git",
    "directory": "packages/ui/auth-fe-bundle"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "source": "src/index.ts",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore --report-unused-disable-directives",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "yarn lint:style --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck",
    "dev": "yarn compile && next dev",
    "build": "yarn compile && next build",
    "start": "yarn compile && next start",
    "lint": "yarn compile && next lint"
  },
  "dependencies": {
    "@fern-ui/ui": "workspace:*"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.7.18",
    "depcheck": "^1.4.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "next": "^14.0.4",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.8.8",
    "stylelint": "^16.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "4.9.4"
  }
}
