    --data-raw '{
        "prompt": "A photo of woman wearing a (rose  pink dress:1)",
        "negative_prompt": "Blurry photo, distortion, low-res, poor quality",
        "controlnet": "octoai:canny_sdxl",
        "width": 1024,
        "height": 1024,
        "num_images": 1,
        "sampler": "DDIM",
        "steps": 30,
        "cfg_scale": 12,
        "use_refiner": true,
