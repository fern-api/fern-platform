{
  "name": "@fern-ui/docs-bundle",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern-platform.git",
    "directory": "packages/ui/docs-bundle"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "source": "src/index.ts",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "pnpm lint:style --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck",
    "env:fern-dev": "env-cmd -r .env-cmdrc.cjs -e fern-dev",
    "env:fern-prod": "env-cmd -r .env-cmdrc.cjs -e fern-prod",
    "dev:fern-dev": "pnpm compile && pnpm env:fern-dev next dev",
    "dev:fern-prod": "pnpm compile && pnpm env:fern-prod next dev",
    "build:fern-dev": "pnpm compile && pnpm env:fern-dev next build",
    "build:fern-prod": "pnpm compile && pnpm env:fern-prod next build",
    "start:fern-dev": "pnpm compile && pnpm env:fern-dev next start",
    "start:fern-prod": "pnpm compile && pnpm env:fern-prod next start",
    "lint:fern-dev": "pnpm compile && pnpm env:fern-dev next lint",
    "lint:fern-prod": "pnpm compile && pnpm env:fern-prod next lint"
  },
  "dependencies": {
    "@fern-api/fdr-sdk": "0.60.0-1-g4795d27",
    "@fern-api/venus-api-sdk": "^0.1.0",
    "@fern-ui/core-utils": "workspace:*",
    "@fern-ui/ui": "workspace:*",
    "@tailwindcss/forms": "^0.5.7",
    "@vercel/edge-config": "^1.1.0",
    "@workos-inc/node": "^6.1.0",
    "cssnano": "^6.0.3",
    "jose": "^5.2.3",
    "jsonpath": "^1.1.1",
    "next": "^14.1.1",
    "postcss-import": "^16.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.3",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "depcheck": "^1.4.3",
    "env-cmd": "toddbluhm/env-cmd",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "organize-imports-cli": "^0.10.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "stylelint": "^16.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "4.9.5"
  }
}
