    "next": "npm:@fern-api/next@14.2.9-fork.0",
    "node-fetch": "2.7.0",
    "postcss-import": "^16.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.3",
    "ts-essentials": "^10.0.1",
    "url-join": "5.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.23.8"
