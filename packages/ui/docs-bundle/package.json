{
  "name": "@fern-ui/docs-bundle",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern-platform.git",
    "directory": "packages/ui/docs-bundle"
  },
  "private": true,
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "vitest --run --passWithNoTests --globals",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "pnpm lint:style --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck",
    "build": "pnpm compile",
    "env:fern-dev": "env-cmd -r .env-cmdrc.cjs -e fern-dev",
    "env:fern-prod": "env-cmd -r .env-cmdrc.cjs -e fern-prod",
    "dev:fern-dev": "pnpm turbo build && NODE_OPTIONS='--inspect' pnpm env:fern-dev next dev",
    "dev:fern-prod": "pnpm turbo build && NODE_OPTIONS='--inspect' pnpm env:fern-prod next dev",
    "build:fern-dev": "pnpm turbo build && pnpm env:fern-dev next build",
    "build:fern-prod": "pnpm turbo build && pnpm env:fern-prod next build",
    "start:fern-dev": "pnpm turbo build && pnpm env:fern-dev next start",
    "start:fern-prod": "pnpm turbo build && pnpm env:fern-prod next start",
    "lint:fern-dev": "pnpm turbo build && pnpm env:fern-dev next lint",
    "lint:fern-prod": "pnpm turbo build && pnpm env:fern-prod next lint"
  },
  "dependencies": {
    "@algolia/requester-fetch": "^4.24.0",
    "@aws-sdk/client-s3": "^3.335.0",
    "@aws-sdk/s3-request-presigner": "^3.574.0",
    "@fern-api/fdr-sdk": "workspace:*",
    "@fern-api/venus-api-sdk": "^0.1.0",
    "@fern-ui/chatbot": "workspace:*",
    "@fern-ui/components": "workspace:*",
    "@fern-ui/core-utils": "workspace:*",
    "@fern-ui/fdr-utils": "workspace:*",
    "@fern-ui/search-utils": "workspace:*",
    "@fern-ui/ui": "workspace:*",
    "@sentry/nextjs": "^7.112.2",
    "@vercel/edge-config": "^1.1.0",
    "@vercel/kv": "^2.0.0",
    "@workos-inc/node": "^6.1.0",
    "algoliasearch": "^4.24.0",
    "cohere-ai": "^7.9.5",
    "cssnano": "^6.0.3",
    "esbuild": "0.20.2",
    "feed": "^4.2.2",
    "form-data": "4.0.0",
    "jsonpath": "^1.1.1",
    "next": "^14.2.4",
    "node-fetch": "2.7.0",
    "path-to-regexp": "^7.0.0",
    "postcss-import": "^16.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.3",
    "ts-essentials": "^10.0.1",
    "url-join": "5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@fern-platform/configs": "workspace:*",
    "@next/bundle-analyzer": "^14.0.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^18.7.18",
    "@types/node-fetch": "2.6.9",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.16",
    "depcheck": "^1.4.3",
    "env-cmd": "toddbluhm/env-cmd",
    "eslint": "^8.56.0",
    "organize-imports-cli": "^0.10.0",
    "postcss": "^8.4.33",
    "prettier": "^3.3.2",
    "sass": "^1.74.1",
    "stylelint": "^16.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "5.4.3",
    "vitest": "^1.5.0"
  }
}
