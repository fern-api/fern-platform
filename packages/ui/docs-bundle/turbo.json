{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build:fern-prod": {
      "dependsOn": ["^build", "^compile"]
    },
    "build:fern-dev": {
      "dependsOn": ["^build", "^compile"]
    },
    "dev:fern-prod": {
      "dependsOn": ["^build", "^compile"]
