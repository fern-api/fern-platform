{
  "name": "@fern-ui/fontawesome-cdn",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern-platform.git",
    "directory": "packages/ui/fontawesome-cdn"
  },
  "private": true,
  "files": [
    "dist"
  ],
