  "optionalDependencies": {
    "@next/swc-linux-x64-gnu": "14.2.9",
    "@next/swc-win32-x64-msvc": "14.2.9",
    "@next/swc-darwin-x64": "14.2.9",
    "@next/swc-darwin-arm64": "14.2.9"
