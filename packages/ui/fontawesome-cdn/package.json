    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@fern-platform/configs": "workspace:*",
    "@next/bundle-analyzer": "14.2.9",
    "@types/node": "^18.7.18",
    "@types/react": "^18.3.5",
    "depcheck": "^1.4.3",
    "eslint": "^8.56.0",
    "next": "npm:@fern-api/next@14.2.9-fork.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^3.3.2",
    "stylelint": "^16.1.0",
    "typescript": "5.4.3",
    "vitest": "^1.5.0"
