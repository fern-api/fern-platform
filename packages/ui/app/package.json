    "@types/lodash-es": "^4.17.12",
    "@types/marked": "^5.0.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/react-test-renderer": "^18.0.6",
    "depcheck": "^1.4.3",
    "eslint": "^8.26.0",
    "jest": "^29.7.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.14.0",
    "typescript": "4.9.4",
    "vite": "^4.3.9"
