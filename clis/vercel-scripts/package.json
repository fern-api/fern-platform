{
  "name": "@fern-platform/vercel-scripts",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/cli.cjs",
  "bin": {
    "vercel-scripts": "dist/cli.cjs"
  },
  "scripts": {
    "clean": "rm -rf ./dist && tsc --build --clean",
    "compile": "tsup ./src/cli.ts --format cjs",
    "vercel-scripts": "pnpm compile && node ./dist/cli.cjs",
    "test": "vitest --run --passWithNoTests --globals --disable-console-intercept",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../.eslintignore",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "pnpm lint:style --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck"
  },
  "devDependencies": {
    "@fern-api/fs-utils": "0.15.0-rc63",
    "@fern-api/github": "workspace:*",
    "@fern-platform/configs": "workspace:*",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.32",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.15",
    "depcheck": "^1.4.3",
    "esbuild": "0.20.2",
    "eslint": "^8.56.0",
    "execa": "^5.1.1",
    "lodash-es": "^4.17.12",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^3.3.2",
    "tmp-promise": "^3.0.3",
    "tsup": "^8.0.2",
    "typescript": "4.9.5",
    "vitest": "^1.5.0",
    "yargs": "^17.4.1"
  },
  "dependencies": {
    "@fern-fern/vercel": "0.0.4607",
    "glob": "^11.0.0",
    "ts-essentials": "^10.0.1",
    "vercel": "^37.4.2"
  }
}
