{
  "name": "@fern-api/generator-cli",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/cli.cjs",
  "source": "src/index.ts",
  "bin": {
    "generator-cli": "dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && tsc --build --clean",
    "compile": "tsup ./src/cli.ts --format cjs && echo '#!/usr/bin/env node' | cat - dist/cli.cjs > dist/tmp && mv dist/tmp dist/cli.cjs",
    "depcheck": "depcheck",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "test": "vitest --run --passWithNoTests --globals --disable-console-intercept"
  },
  "devDependencies": {
    "@fern-api/fs-utils": "0.15.0-rc63",
    "@fern-platform/github": "workspace:*",
    "@fern-platform/tsconfig": "workspace:*",
    "@types/jest": "^29.5.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.32",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.15",
    "depcheck": "^1.4.3",
    "esbuild": "0.20.2",
    "execa": "^5.1.1",
    "lodash-es": "^4.17.12",
    "organize-imports-cli": "^0.10.0",
    "tmp-promise": "^3.0.3",
    "tsup": "^8.0.2",
    "typescript": "4.9.5",
    "vitest": "^1.5.0",
    "yargs": "^17.4.1"
  }
}
